{
    "name": "Python",
    "extensions": [".py", ".pyw", ".pyx", ".pyi", ".rpy"],
    "case_sensitive": true,
    "detect_numbers": true,
    "line_comment": "#",
    "block_comment_start": "",
    "block_comment_end": "",
    "string_delimiters": ["\""],
    "string2_delimiters": ["'"],
    "property_pattern": false,
    "operators": ["+", "-", "*", "/", "//", "%", "**", "=", "==", "!=", "<", ">", "<=", ">=", "+=", "-=", "*=", "/=", "//=", "%=", "**=", "&", "|", "^", "~", "<<", ">>", "&=", "|=", "^=", "<<=", ">>=", "and", "or", "not", "in", "is", "(", ")", "[", "]", "{", "}", ",", ":", ".", ";", "@", "->"],
    "keywords1": ["False", "None", "True", "and", "as", "assert", "async", "await", "break", "class", "continue", "def", "del", "elif", "else", "except", "finally", "for", "from", "global", "if", "import", "in", "is", "lambda", "nonlocal", "not", "or", "pass", "raise", "return", "try", "while", "with", "yield"],
    "keywords2": ["self", "cls"],
    "keywords3": ["__init__", "__new__", "__del__", "__repr__", "__str__", "__call__", "__len__", "__getitem__", "__setitem__", "__delitem__", "__iter__", "__next__", "__enter__", "__exit__", "__name__", "__main__"],
    "keywords4": ["int", "float", "str", "list", "dict", "tuple", "set", "bool", "bytes", "bytearray", "object", "type", "range", "enumerate", "zip", "map", "filter", "sorted", "reversed", "abs", "all", "any", "len", "min", "max", "sum", "print", "input", "open", "Exception", "ValueError", "TypeError", "KeyError", "IndexError", "AttributeError"],
    "styles": {
        "Default": {
            "color": "#000000",
            "background": "#FFFFFF",
            "bold": false,
            "italic": false
        },
        "Keyword1": {
            "color": "#0000FF",
            "background": "#FFFFFF",
            "bold": true,
            "italic": false
        },
        "Keyword2": {
            "color": "#267F99",
            "background": "#FFFFFF",
            "bold": false,
            "italic": false
        },
        "Keyword3": {
            "color": "#795E26",
            "background": "#FFFFFF",
            "bold": false,
            "italic": false
        },
        "Keyword4": {
            "color": "#267F99",
            "background": "#FFFFFF",
            "bold": false,
            "italic": false
