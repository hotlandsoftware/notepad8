            "background": "#FFFFFF",
            "bold": false,
            "italic": false
        },
        "Operator": {
            "color": "#000000",
            "background": "#FFFFFF",
            "bold": false,
            "italic": false
        },
        "String": {
            "color": "#A31515",
            "background": "#FFFFFF",
